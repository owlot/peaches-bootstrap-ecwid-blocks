{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "peaches/ecwid-product-ingredients",
	"version": "0.1.0",
	"title": "Bootstrap ECWID Product Ingredients",
	"category": "peaches-bootstrap",
	"ancestor": [ "peaches/ecwid-product-detail" ],
	"description": "Display product ingredients for ECWID products with Bootstrap accordion styling.",
	"keywords": [
		"bootstrap",
		"product",
		"ingredients",
		"accordion",
		"peaches",
		"ecwid"
	],
	"textdomain": "peaches",
	"supports": {
		"html": false,
		"layout": false,
		"color": {
			"overlay": true
		},
		"interactivity": true
	},
	"attributes": {
		"startOpened": {
			"type": "boolean",
			"default": false
		}
	},
	"viewScriptModule": "file:./view.js",
	"editorScript": "file:./index.js"
}
