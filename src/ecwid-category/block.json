{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "peaches/ecwid-category",
	"version": "0.1.0",
	"title": "Bootstrap ECWID category",
	"category": "peaches-bootstrap",
	"description": "A ECWID category bootstrap themed block.",
	"keywords": [ "bootstrap", "grid", "peaches", "category", "ecwid" ],
	"textdomain": "peaches",
	"supports": {
		"html": false,
		"layout": false,
		"color": {
			"overlay": true
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "peaches-ecwid-category-editor",
	"attributes": {
		"classes": {
			"type": "string"
		},
		"xs": {
			"type": "object",
			"default": {
				"rowCols": 4
			}
		},
		"sm": {
			"type": "object",
			"default": {}
		},
		"md": {
			"type": "object",
			"default": {}
		},
		"lg": {
			"type": "object",
			"default": {}
		},
		"xl": {
			"type": "object",
			"default": {}
		},
		"xxl": {
			"type": "object",
			"default": {}
		}
	}
}
